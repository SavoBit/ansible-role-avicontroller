#!/bin/sh
#
#       /etc/init.d/avicontroller
#
#       Daemon for avicontroller
#
# chkconfig:   2345 99 99
# description: Avi Controller

### BEGIN INIT INFO
# Provides:       avicontroller
# Required-Start: docker
# Required-Stop:
# Should-Start:
# Should-Stop:
# Default-Start: 2 3 4 5
# Default-Stop:  0 1 6
# Short-Description: start and stop avicontroller
# Description: Avi Controller
### END INIT INFO

case "$1" in
  start|stop|restart|status)
    echo "Upstart is used on this system."
    echo "Please use Upstart to manage the process and runlevel."
    echo "Running: initctl ${1} avicontroller"
    /sbin/initctl $1 avicontroller
    ;;
  *)
    echo $"Usage: $0 {start|stop|status|restart}"
    exit 2
esac

exit $?
